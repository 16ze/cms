    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.6",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "dotenv": "^16.4.7",
    "googleapis": "^162.0.0",
