    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.10.1",
    "@axe-core/react": "^4.10.1",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.34.3",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "20.19.20",
    "@types/node-cron": "^3.0.11",
    "@types/pino-http": "^6.1.0",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "19.2.2",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "chrome-launcher": "^1.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^10.1.8",
    "lighthouse": "^12.5.1",
    "prettier": "^3.6.2",
