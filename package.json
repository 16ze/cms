{
  "name": "kairowebsite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --no-lint",
    "start": "next start -p $PORT",
    "heroku-postbuild": "prisma generate && next build --no-lint",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "playwright test",
    "test:e2e": "playwright test",
    "test:unit": "playwright test --grep @unit || echo 'No unit tests configured'",
    "test:integration": "playwright test --grep @integration || playwright test",
    "test:stability": "playwright test tests/e2e/isolation.spec.ts tests/e2e/auth.spec.ts tests/e2e/crud.spec.ts tests/e2e/dashboard.spec.ts",
    "test:security:e2e": "playwright test tests/e2e/rate-limiting.spec.ts tests/e2e/validation-zod.spec.ts tests/e2e/security-headers.spec.ts tests/e2e/isolation.spec.ts",
    "test:security:all": "npm run test:security && npm run test:security:e2e",
    "test:isolation": "playwright test tests/e2e/isolation.spec.ts",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:accessibility": "ts-node scripts/test-accessibility.ts",
    "test:performance": "ts-node scripts/test-performance.ts",
    "test:security": "ts-node scripts/test-security.ts",
    "migrate:routes": "tsx scripts/migrate-to-safe-handler.ts",
    "postinstall": "prisma generate && npm run seed:auto || echo 'Seed automatique ignoré (base non initialisée)'",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --project tsconfig.strict.json --noEmit",
    "check:env": "node scripts/check-env.ts",
    "check:env:complete": "tsx scripts/check-env-complete.ts",
    "init": "node scripts/init-project.js",
    "configure": "node scripts/configure-content.js",
    "clean": "node scripts/clean-fallbacks.js",
    "seed:clean": "ts-node prisma/seeds/clean-seed.ts",
    "seed:auto": "tsx prisma/seeds/auto-seed-tenants.ts",
    "audit:security": "npm audit --production",
    "lint:strict": "eslint . --ext .ts,.tsx --max-warnings=0",
    "report:audit": "tsx scripts/generate-security-report.ts",
    "check:sentry": "tsx scripts/check-sentry.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@google-analytics/data": "^5.2.0",
